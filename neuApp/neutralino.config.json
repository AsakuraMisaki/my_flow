{
  "applicationId": "js.neutralino.workflow",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/src/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "filesystem.*",
    "os.*"
  ],
  "modes": {
    "window": {
      "injectGlobals": true,
      "title": "neuApp",
      "width": 1600,
      "height": 900,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/src/res/icon.png"
    }
  },
  "cli": {
    "binaryName": "neuApp",
    "resourcesPath": "/src/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "5.5.0",
    "clientVersion": "5.5.0",
    "frontendLibrary": {
      "patchFile": "/src/dist/index.html",
      "devUrl": "http://127.0.0.1:5173/",
      "projectPath": "/src/",
      "devCommand": "npm run dev",
      "buildCommand": "npm run build"
    }
  }
}